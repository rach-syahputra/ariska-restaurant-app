{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["standard"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "parser": "@babel/eslint-parser",
  "rules": {
    "space-before-function-paren": "off",
    "no-new": "off",
    "eol-last": "off"
  }
}
